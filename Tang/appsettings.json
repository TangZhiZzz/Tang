{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "DbConfig": {
    "ConnectionString": "DataSource=tang.db",
    "DbType": "Sqlite",
    "IsAutoCloseConnection": true
  },
  "JwtConfig": {
    "SecretKey": "This is a very long secret key for jwt",
    "Issuer": "Tang",
    "Audience": "Tang",
    "ExpireMinutes": 1440
  },
  "CacheConfig": {
    "Type": "Memory",
    "RedisConnection": "localhost:6379"
  },
  "LogConfig": {
    "FilePath": "logs/log-.txt",
    "RetainedDays": 30,
    "WriteToConsole": true
  }
}
